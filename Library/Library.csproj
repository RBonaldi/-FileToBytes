<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>FileToBytes</PackageId>
    <Version>0.0.0.10</Version>
    <Authors>Rafael Bonaldi - rafaelbonaldi@gmail.com</Authors>
    <Description>Convert file into byte array.
Convert byte array into file.
Validate file.</Description>
    <PackageProjectUrl>https://github.com/RBonaldi/-FileToBytes</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/RBonaldi/-FileToBytes</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>FileToBytes, BytesToFile, ValidateBytes, ValidateFile, Bytes[], Bytes, File</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Assets\logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Features" Version="5.0.15" />
  </ItemGroup>

</Project>
